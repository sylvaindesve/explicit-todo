{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules"
  ],
  "jsRules": {},
  "rules": {
    "interface-name": ["never-prefix"],
    "quotemark": [true, "single", "avoid-escape"],
    "variable-name": [
      true,
      "allow-leading-underscore",
      "ban-keywords"
    ]
  },
  "rulesDirectory": []
}
